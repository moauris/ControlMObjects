<Window x:Class="ControlM_Manager_GUI.View.MachinePicker"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ControlM_Manager_GUI.View"
        mc:Ignorable="d" WindowStyle="ThreeDBorderWindow"
        Title="MachinePicker" Height="450" Width="800">
    <StackPanel Orientation="Vertical">
        <ListBox Margin="20" Height="350">
            
        </ListBox>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="1*"/>
                <ColumnDefinition Width="1*"/>
                <ColumnDefinition Width="1*"/>
                <ColumnDefinition Width="1*"/>
                <ColumnDefinition Width="1*"/>
                <ColumnDefinition Width="1*"/>
            </Grid.ColumnDefinitions>
            <Button Grid.Column="1">选择</Button>
            <Button Grid.Column="4">取消</Button>
        </Grid>
    </StackPanel>
</Window>
